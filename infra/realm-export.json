{
  "id": "ArmeniaBank",
  "realm": "ArmeniaBank",
  "enabled": true,
  "displayName": "Armenia Bank Realm",
  "loginTheme": "keycloak",
  "sslRequired": "none",
  "registrationAllowed": true,
  "editUsernameAllowed": false,
  "resetPasswordAllowed": true,
  "verifyEmail": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "defaultDefaultClientScopes": [
    "profile",
    "email",
    "roles",
    "web-origins"
  ],
  "clients": [
    {
      "clientId": "backend-gateway-client",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "gateway-service",
      "redirectUris": [
        "http://localhost:9090/*"
      ],
      "webOrigins": ["+"],
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true,
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "attributes": {
        "access.token.lifespan": "300",
        "refresh.token.lifespan": "1800"
      }
    },
    {
      "clientId": "auth-service",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "auth-service",
      "redirectUris": [],
      "webOrigins": ["+"],
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "attributes": {
        "access.token.lifespan": "300",
        "refresh.token.lifespan": "1800"
      }
    },
    {
      "clientId": "audit-service",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "audit-service",
      "redirectUris": [],
      "webOrigins": ["+"],
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "attributes": {
        "access.token.lifespan": "300",
        "refresh.token.lifespan": "1800"
      }
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "admin",
        "description": "Administrator role"
      },
      {
        "name": "user",
        "description": "Regular user role"
      },
      {
        "name": "audit",
        "description": "Audit logger role"
      },
      {
        "name": "realm-management/manage-users",
        "description": "Manage users via Admin API"
      }
    ],
    "client": {
      "auth-service": [
        {
          "name": "service-role",
          "description": "Auth-service role"
        }
      ]
    }
  },
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "email": "admin@armeniabank.com",
      "firstName": "Admin",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "admin",
          "temporary": false
        }
      ],
      "realmRoles": [
        "admin",
        "realm-management/manage-users"
      ],
      "emailVerified": true
    },
    {
      "username": "auditUser",
      "enabled": true,
      "email": "audituser@armeniabank.com",
      "firstName": "Audit",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "audit123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "audit"
      ],
      "emailVerified": true
    },
    {
      "username": "user@example.com",
      "enabled": true,
      "email": "user@example.com",
      "firstName": "User",
      "lastName": "Example",
      "credentials": [
        {
          "type": "password",
          "value": "user123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user"
      ],
      "emailVerified": true,
      "attributes": {
        "locale": "en"
      }
    }
  ]
}
